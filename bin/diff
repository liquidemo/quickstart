#!/bin/bash

java -jar lib/liquibase.jar \
      --driver=com.mysql.jdbc.Driver \
      --classpath=lib/mysql-connector-java-5.1.19.jar:lib/h2-1.3.166.jar \
      --url=jdbc:mysql://localhost/menagerie2 \
      --username=menagerie \
      --password=menagerie \
      --referenceUrl=jdbc:mysql://localhost/menagerie \
      --referenceDriver=com.mysql.jdbc.Driver \
      --referenceUsername=menagerie \
      --referencePassword=menagerie \
      diff

